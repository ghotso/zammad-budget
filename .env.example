# Node Environment
NODE_ENV=development

# Server Configuration
PORT=3000

# Security
JWT_SECRET=your-secret-key-here
APP_PASSWORD=admin

# Database Configuration
DATABASE_URL=file:./data/dev.db

# Zammad Integration
ZAMMAD_URL=http://your-zammad-instance
ZAMMAD_TOKEN=your-zammad-token

# CORS Settings (Development)
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:8071,http://localhost:3071,http://127.0.0.1:5173,http://127.0.0.1:8071,http://127.0.0.1:3071

# Cookie Settings
COOKIE_SECURE=false
COOKIE_SAMESITE=Lax

# Docker Settings (Production)
# DATABASE_URL=file:/data/dev.db
# COOKIE_SECURE=true
# COOKIE_SAMESITE=Strict
# CORS_ALLOWED_ORIGINS=http://your-domain.com

# Debug Settings
DEBUG_LEVEL=info
ENABLE_REQUEST_LOGGING=true